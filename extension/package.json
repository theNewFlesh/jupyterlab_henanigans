{
  "name": "@theNewFlesh/jupyterlab_henanigans",
  "version": "0.2.1",
  "description": "A dark, easy-on-the-eyes theme for JupyterLab.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/theNewFlesh/jupyterlab_henanigans",
  "bugs": {
    "url": "https://github.com/theNewFlesh/jupyterlab_henanigans/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Alex Braun",
    "email": "alexander.g.braun@gmail.com"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js",
    "src/**/*.{ts,tsx}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/theNewFlesh/jupyterlab_henanigans.git"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.1.2"
  },
  "devDependencies": {
    "rimraf": "~5.0.5",
    "typedoc": "~0.24.7",
    "typescript": "~5.1.6"
  },
  "sideEffects": true,
  "publishConfig": {
    "access": "public"
  },
  "styleModule": "style/index.js",
  "jupyterlab": {
    "extension": true,
    "themePath": "style/theme.css"
  },
  "directories": {
    "lib": "lib/"
  }
}
